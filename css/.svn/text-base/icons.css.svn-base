* { background-repeat: no-repeat; background-position: left center; }

.ico * { border: none !important; }

.ico, .button span, button span {
	padding: 2px 0 4px 20px;
	white-space: nowrap;
	background-position: 0 3px;
	background-repeat: no-repeat !important;
	overflow: visible;
	cursor: pointer;
}

td.icon span.ico, th.icon span.ico { margin: -2px 0 0 0 !important; float: left; width: 20px; }

img.ico { margin: 0 0 -3px 0; border: none; padding: 0; }

input { 
	width: 18px; 
	margin: -2px 0 0 0; 
	height: 18px; 
	background-color: transparent; 
	border: none;
}

ul { background-image: none !important; }
ul li { background-repeat: no-repeat; background-position: top left !important; }

