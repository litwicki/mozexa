input.delete, input.approve { width: 20px; }
input.delete:hover { background-image: url(/images/icons/fugue/cross.png); }

.close, .cancel { 
	background-image: url(/images/icons/fugue/cross-circle.png); 
	background-position: 0 2px; 
	background-repeat: no-repeat; 
}

.close:hover, .cancel:hover, button:hover .close, button:hover .cancel, .button:hover .cancel, .button:hover .close { 
	background-image: url(/images/icons/fugue/cross-circle-frame.png); 
	background-position: 0 2px; 
	background-repeat: no-repeat;
	opacity: 1.0;
}

*+html button.close, *+html button.cancel, *+html button.close:hover, *+html button.cancel:hover { background-position: 0 10px; }

.check, .approve, button .check, button .approve { 
	background-image: url(/images/icons/fugue/tick-circle.png); 
}

.check:hover, .approve:hover, button:hover .approve, button:hover .check, .button:hover .check, .button:hover .approve { 
	background-image: url(/images/icons/fugue/tick-circle-frame.png);
